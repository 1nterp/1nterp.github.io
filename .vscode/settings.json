{
    "[markdown]": {
    },
    "frontMatter.content.fmHighlight": true,
    "frontMatter.taxonomy.contentTypes": [
        {
            "name": "default",
            "previewPath": null,
            "pageBundle": false,
            "fields": [
            {
                "title": "Title",
                "name": "title",
                "type": "string"
            },
            {
                "title": "Publishing date",
                "name": "date",
                "type": "datetime",
                "default": "{{now}}",
