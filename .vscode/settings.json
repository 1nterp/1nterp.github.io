            },
            {
                "title": "Article preview",
                "name": "preview",
                "type": "image"
            },
            {
                "title": "Is in draft",
                "name": "draft",
                "type": "draft"
            },
            {
                "title": "Categories",
                "name": "categories",
                "type": "categories"
            },
            {
                "title": "Tags",
                "name": "tags",
                "type": "tags"
            }
            ]
        }
    ],
    "frontMatter.taxonomy.dateFormat": "yyyy-MM-dd HH:mm:ss XX",
    "frontMatter.framework.startCommand": "hugo server"
